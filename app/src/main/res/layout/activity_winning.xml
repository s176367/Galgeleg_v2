<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FA08"
    tools:context=".Winning">

    <TextView
        android:id="@+id/textView6"
        android:layout_width="321dp"
        android:layout_height="238dp"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginTop="36dp"
        android:gravity="center"
        android:text="Tillykke!! Du har vundet! Du  kan gemme din highscore forneden"
        android:textSize="35dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/menu"
        android:layout_width="119dp"
        android:layout_height="73dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="44dp"
        android:layout_marginRight="44dp"
        android:text="Hovedmenu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antalforsøgInt" />

    <Button
        android:id="@+id/highscore"
        android:layout_width="119dp"
        android:layout_height="73dp"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginTop="84dp"
        android:text="Highscore"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antalforsøgInt" />

    <TextView
        android:id="@+id/antalForsøgtext"
        android:layout_width="202dp"
        android:layout_height="72dp"
        android:layout_marginStart="104dp"
        android:layout_marginLeft="104dp"
        android:gravity="center"
        android:text="Antal forkerte gæt"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/antalforsøgInt"
        android:layout_width="141dp"
        android:layout_height="63dp"
        android:layout_marginEnd="132dp"
        android:layout_marginRight="132dp"
        android:gravity="center"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antalForsøgtext" />

</androidx.constraintlayout.widget.ConstraintLayout>